{% extends "layouts/base.njk" %}

{% block content %}
    <!-- Breadcrumb area -->
    <div class="rts-bread-crumb-area bg_image bg-breadcrumb">
        <div class="container ptb--65">
            <div class="row">
                <div class="col-lg-12">
                    <div class="con-tent-main">
                        <div class="wrapper">
                            <span class="bg-text-stok">{{ title }}</span>
                            <div class="title skew-up">
                                <a href="#">{{ title }}</a>
                            </div>
                            <div class="slug skew-up">
                                <a href="/">HOME /</a>
                                <a class="active" href="{{ page.url }}">{{ title }}</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="inner-page-wrapper">
        {{ content | safe }}
    </div>
{% endblock %}
