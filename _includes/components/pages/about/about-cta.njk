<!-- rts call to action area start -->
<div class="rts-call-to-action rts-section-gapBottom">
    <div class="container">
        <div class="row g-24">
            <!-- Phone CTA -->
            <div class="col-lg-6">
                <div class="cta-main-wrapper-one">
                    <div class="icon">
                        <img src="data:image/svg+xml;base64,">
                    </div>
                    <div class="info-area">
                        <span>Call Us For Service</span>
                        <a href="tel:{{ site.contact.phone | replace(" ", "") }}">
                            <h4 class="title">{{ site.contact.phone }}</h4>
                        </a>
                        <!-- <p>Call 24/7 Anytime For Your Problem</p> -->
                    </div>
                </div>
            </div>

            <!-- Email CTA -->
            <div class="col-lg-6">
                <div class="cta-main-wrapper-one cta_primary">
                    <div class="icon">
                        <img src="data:image/svg+xml;base64,">
                    </div>
                    <div class="info-area">
                        <span>Mail Us Anytime</span>
                        <a href="mailto:{{ site.contact.email }}">
                            <h4 class="title">{{ site.contact.email }}</h4>
                        </a>
                        <!-- <p>Mail 24/7 Anytime For Your Problem</p> -->
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- rts call to action area end -->
