<!-- header style two -->
<div class="header-header-two">
    <!-- header- solaric two -->
    <div class="header-two-solari header-top">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="header-top-m">
                        <div class="left">
                            <div class="inf">
                                <i class="fa-regular fa-clock"></i>
                                <p>Mon - Fri 8:00 - 18:00 / Sunday 8:00 - 14:00</p>
                            </div>
                            <div class="inf">
                                <i class="fa-regular fa-envelope"></i>
                                <a href="tel:+4733378901">info@zeronetcleanenergy.com</a>
                            </div>
                        </div>
                        <div class="right">
                            <div class="social-header-top-h2">
                                <span>Visit Us:</span>
                                <ul>
                                    <li><a href="#"><i class="fa-brands fa-facebook-f"></i></a></li>
                                    <li><a href="#"><i class="fa-brands fa-twitter"></i></a></li>
                                    <li><a href="#"><i class="fa-brands fa-linkedin-in"></i></a></li>
                                    <li><a href="#"><i class="fa-brands fa-instagram"></i></a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- header main -->
    <div class="header-main-h2 header--sticky">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="main-haeder-wrapper-h2">
                        <a href="/" class="logo-area">
                            <img src="/assets/images/logo/logo-01.png" alt="logo">
                        </a>

                        <div class="header-nav main-nav-one">
                            <nav>
                                {% set navPages = collections.all | eleventyNavigation %}
                                <ul>
                                    {%- for entry in navPages %}
                                    <li>
                                        <a class="nav-link{% if entry.url == page.url %} active{% endif %}" href="{{ entry.url | url }}">{{ entry.title | upper }}</a>
                                    </li>
                                    {%- endfor %}
                                </ul>
                            </nav>
                        </div>

                        <div class="actions-area">
                            <div class="menu-btn" id="menu-btn">
                                <svg width="20" height="16" viewBox="0 0 20 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                                    <rect y="14" width="20" height="2" fill="#4AAB3D"/>
                                    <rect y="7" width="20" height="2" fill="#4AAB3D"/>
                                    <rect width="20" height="2" fill="#4AAB3D"/>
                                </svg>
                            </div>
                            <a href="/appointments" class="rts-btn btn-primary">Request an Appointment</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<!-- Side bar -->
